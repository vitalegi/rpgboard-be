{
  "security": {
    "allowedMethods": [
      "GET",
      "POST",
      "OPTIONS"
    ],
    "allowedHeaders": [
      "Content-Type",
      "Access-Control-Request-Method",
      "Access-Control-Allow-Credentials",
      "Access-Control-Allow-Origin",
      "Access-Control-Allow-Headers",
      "x-requested-with",
      "Authorization",
      "origin",
      "accept"
    ],
    "allowCredentials": true,
    "auth": "FIREBASE"
  },
  "database": {
    "sslMode": "REQUIRED"
  }
}